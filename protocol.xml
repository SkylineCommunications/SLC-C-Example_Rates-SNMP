<Protocol xmlns="http://www.skyline.be/protocol">
	<Name>SLC SDF Rates - SNMP</Name>
	<Description>SLC SDF Rates - SNMP DataMiner driver</Description>
	<Version>1.0.0.1</Version>
	<IntegrationID>DMS-DRV-0000</IntegrationID>
	<Provider>Skyline Communications</Provider>
	<Vendor>Skyline</Vendor>
	<VendorOID>1.3.6.1.4.1.8813.2.48</VendorOID>
	<DeviceOID>998</DeviceOID>
	<ElementType>Training Example</ElementType>
	<Type relativeTimers="true">snmpv2</Type>
	<!-- SuppressValidator 1.22.6 No WebInterface required for this example driver. -->
	<Display defaultPage="General" pageOrder="General" wideColumnPages="General"/>
	<!-- /SuppressValidator 1.22.6 -->
	<SNMP includepages="true">auto</SNMP>

	<Compliancies>
		<CassandraReady>true</CassandraReady>
		<MinimumRequiredVersion>10.0.0.0 - 11025</MinimumRequiredVersion>
	</Compliancies>

	<Params>
		<Param id="2">
			<Name>afterStartup</Name>
			<Description>After Startup</Description>
			<Type>dummy</Type>
		</Param>
		<Param id="3">
			<Name>dummyForQA3</Name>
			<Description>Dummy For QA3</Description>
			<Type>dummy</Type>
		</Param>
		
		<!-- Streams Table -->
		<Param id="1000">
			<Name>streams</Name>
			<Description>Streams</Description>
			<Type>array</Type>
			<ArrayOptions index="0">
				<ColumnOption idx="0" pid="1001" type="snmp" options=""/>
				<ColumnOption idx="1" pid="1002" type="snmp" options="" />
				<ColumnOption idx="2" pid="1003" type="snmp" options=";disableHeaderSum;disableHistogram;disableHeatmap" />
				<ColumnOption idx="3" pid="1004" type="retrieved" options=";disableHeaderSum;disableHistogram;disableHeatmap" />
				<ColumnOption idx="4" pid="1005" type="retrieved" options=";save" />
			</ArrayOptions>
			<Information>
				<Subtext>Streams Table</Subtext>
			</Information>
			<SNMP>
				<Enabled>true</Enabled>
				<OID type="complete" options=";instance;multipleGetNext"></OID>
				<!--<OID type="complete" options=";instance;multipleGetNext">1.3.6.1.2.1.1.100</OID>-->
			</SNMP>
			<Display>
				<RTDisplay>true</RTDisplay>
				<Positions>
					<Position>
						<Page>General</Page>
						<Column>0</Column>
						<Row>0</Row>
					</Position>
				</Positions>
			</Display>
			<Measurement>
				<Type options="tab=columns:1001|0-1002|1-1003|2-1005|4-1004|3,width:97-275-130-0-140,sort:STRING-STRING-INT-STRING-INT,lines:25,filter:true">table</Type>
			</Measurement>
		</Param>
		<Param id="1001" trending="false">
			<Name>streamsIndex</Name>
			<Description>Index [IDX] (Streams)</Description>
			<Type>read</Type>
			<Information>
				<Subtext>This is the key used internally by DataMiner to identify the table entries.</Subtext>
			</Information>
			<Interprete>
				<RawType>other</RawType>
				<Type>string</Type>
				<LengthType>next param</LengthType>
			</Interprete>
			<Display>
				<RTDisplay>true</RTDisplay>
			</Display>
			<Measurement>
				<Type>string</Type>
			</Measurement>
		</Param>
		<Param id="1002" trending="false">
			<Name>streamsDrescription</Name>
			<Description>Description (Streams)</Description>
			<Type>read</Type>
			<Information>
				<Subtext>A textual description for the stream.</Subtext>
			</Information>
			<Interprete>
				<RawType>other</RawType>
				<Type>string</Type>
				<LengthType>next param</LengthType>
			</Interprete>
			<SNMP>
				<Enabled>true</Enabled>
				<OID type="complete">1.3.6.1.2.1.1.100.1.2</OID>
				<Type>octetstring</Type>
			</SNMP>
			<Display>
				<RTDisplay>true</RTDisplay>
			</Display>
			<Measurement>
				<Type>string</Type>
			</Measurement>
		</Param>
		<Param id="1003" trending="true">
			<Name>streamsOctetsCounter</Name>
			<Description>Octets Counter (Streams)</Description>
			<Type>read</Type>
			<Information>
				<Subtext>The octets counter for the stream.</Subtext>
			</Information>
			<Interprete>
				<RawType>numeric text</RawType>
				<Type>double</Type>
				<LengthType>next param</LengthType>
			</Interprete>
			<SNMP>
				<Enabled>true</Enabled>
				<OID type="complete">1.3.6.1.2.1.1.100.1.3</OID>
				<Type>integer</Type>
			</SNMP>
			<!-- SuppressValidator 2.5.1 Thresholds fully depending on users -->
			<Alarm>
				<Monitored>true</Monitored>
			</Alarm>
			<!-- /SuppressValidator 2.5.1 -->
			<Display>
				<RTDisplay>true</RTDisplay>
				<Units>Octets</Units>
				<Range>
					<Low>0</Low>
				</Range>
			</Display>
			<Measurement>
				<Type>number</Type>
			</Measurement>
		</Param>
		
		<Param id="1004" trending="true">
			<Name>streamsBitRate</Name>
			<Description>Bit Rate (Streams)</Description>
			<Type>read</Type>
			<Information>
				<Subtext>The calculated bit rate based on the octets counter and delta time between SNMP group executions.</Subtext>
			</Information>
			<Interprete>
				<RawType>numeric text</RawType>
				<Type>double</Type>
				<LengthType>next param</LengthType>
				<Exceptions>
					<Exception id="1" value="-1">
						<Display state="disabled">N/A</Display>
						<Value>-1</Value>
					</Exception>
				</Exceptions>
			</Interprete>
			<!-- SuppressValidator 2.5.1 Thresholds fully depending on users -->
			<Alarm>
				<Monitored>true</Monitored>
			</Alarm>
			<!-- /SuppressValidator 2.5.1 -->
			<Display>
				<RTDisplay>true</RTDisplay>
				<Decimals>3</Decimals>
				<Range>
					<Low>0</Low>
				</Range>
				<Units>bps</Units>
			</Display>
			<Measurement>
				<Type>number</Type>
			</Measurement>
		</Param>
		<Param id="1005" trending="false">
			<Name>streamsBitRateData</Name>
			<Description>Bit Rate Data (Streams)</Description>
			<Type>read</Type>
			<Information>
				<Subtext>Used as a buffer parameter containing data necessary for bit rate calculations based on delta time between SNMP group executions.</Subtext>
			</Information>
			<Interprete>
				<RawType>other</RawType>
				<Type>string</Type>
				<LengthType>next param</LengthType>
			</Interprete>
			<Display>
				<RTDisplay>true</RTDisplay>
			</Display>
			<Measurement>
				<Type>string</Type>
			</Measurement>
		</Param>
	</Params>

	<QActions>
		<QAction id="1" name="Precompiled Code" encoding="csharp" options="precompile">
		</QAction>
		<QAction id="2" name="After Startup" encoding="csharp" triggers="2">
		</QAction>
		
		<QAction id="1000" name="Streams Changes" encoding="csharp" triggers="1000">
		</QAction>
	</QActions>

	<Groups>
		<Group id="1">
			<Name>SNMP Ping Group</Name>
			<Description>SNMP Ping Group</Description>
			<Type>poll</Type>
			<Content>
				<Param>1000</Param>
			</Content>
		</Group>
		<Group id="2">
			<Name>After Startup</Name>
			<Description>After Startup</Description>
			<Type>poll action</Type>
			<Content>
				<Action>2</Action>
			</Content>
		</Group>
		<Group id="1000">
			<Name>Poll Streams</Name>
			<Description>Poll Streams</Description>
			<Type>poll</Type>
			<Content>
				<Param>1000</Param>
			</Content>
		</Group>
	</Groups>

	<Triggers>
		<Trigger id="1">
			<Name>After Startup</Name>
			<On>protocol</On>
			<Time>after startup</Time>
			<Type>action</Type>
			<Content>
				<Id>1</Id>
			</Content>
		</Trigger>
	</Triggers>

	<Actions>
		<Action id="1">
			<Name>After Startup Group</Name>
			<On id="2">group</On>
			<Type>execute next</Type>
		</Action>
		<Action id="2">
			<Name>After Startup QAction</Name>
			<On id="2">parameter</On>
			<Type>run actions</Type>
		</Action>
		<Action id="3">
			<Name>Run QA3</Name>
			<On id="3">parameter</On>
			<Type>run actions</Type>
		</Action>
	</Actions>

	<Timers>
		<Timer id="1">
			<Name>Fast Timer (10s)</Name>
			<Time initial="true">10000</Time>
			<Interval>75</Interval>
			<Content>
				<Group>1000</Group>
			</Content>
		</Timer>
	</Timers>

	<PortSettings name="SNMP Connection">
		
	</PortSettings>

	<VersionHistory>
		<Branches>
			<Branch id="1">
				<Comment>Main Branch</Comment>
				<SystemVersions>
					<SystemVersion id="0">
						<MajorVersions>
							<MajorVersion id="0">
								<MinorVersions>
									<MinorVersion id="1">
										<Date>2022-02-25</Date>
										<Provider>
											<Author>SVD</Author>
											<Company>Skyline Communications</Company>
										</Provider>
										<Changes>
											<NewFeature>Initial version</NewFeature>
										</Changes>
										<References>
											<TaskId>172807</TaskId>
										</References>
									</MinorVersion>
								</MinorVersions>
							</MajorVersion>
						</MajorVersions>
					</SystemVersion>
				</SystemVersions>
			</Branch>
		</Branches>
	</VersionHistory>

</Protocol>